{
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4.0",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "laminas/laminas-config-aggregator": "^1.2",
        "laminas/laminas-diactoros": "^2.2",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^3.9",
        "ramsey/uuid-doctrine": "^1.6",
        "slim/psr7": "^0.6.0",
        "slim/slim": "^4.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/validator": "^5.0",
        "tuupola/slim-jwt-auth": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    }
}
